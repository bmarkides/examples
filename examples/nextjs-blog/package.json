{
  "name": "flamelink-nextjs-blog",
  "description": "This is an example blog using Next.js with Flamelink.",
  "private": true,
  "license": "MIT",
  "keywords": [
    "flamelink",
    "next.js",
    "firebase",
    "firestore",
    "headless",
    "cms",
    "javascript",
    "sdk",
    "web",
    "node",
    "blog",
    "real-time"
  ],
  "dependencies": {
    "@material-ui/core": "^3.9.2",
    "firebase": "^5.9.0",
    "firebase-admin": "^7.1.0",
    "flamelink": "^1.0.0-alpha.2",
    "next": "^8.0.3",
    "next-routes": "^1.4.2",
    "next-seo": "^1.11.1",
    "react": "^16.8.4",
    "react-dom": "^16.8.4"
  },
  "devDependencies": {
    "dotenv-webpack": "^1.7.0",
    "eslint": "^5.15.2",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.4",
    "eslint-plugin-react-hooks": "^1.5.1",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.5",
    "prettier": "^1.16.4",
    "projectz": "^1.7.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run generate-readme && git add README.md && lint-staged"
    }
  },
  "scripts": {
    "dev": "node server.js",
    "build": "next build",
    "start": "NODE_ENV=production node server.js",
    "lint": "eslint '**/*.js' --cache",
    "lint:fix": "yarn lint --fix",
    "generate-readme": "projectz compile"
  },
  "author": "Flamelink <feedback@flamelink.io> (http://flamelink.io)",
  "maintainers": [
    "De Wet van der Merwe <dw@flamelink.io> (https://github.com/dewetvdm)",
    "Jordan Hunt <jordan@flamelink.io> (https://github.com/jordanch)",
    "JP Erasmus <jp@flamelink.io> (http://jperasmus.me)"
  ],
  "contributors": [
    "De Wet van der Merwe <dw@flamelink.io> (https://github.com/dewetvdm)",
    "Jordan Hunt <jordan@flamelink.io> (https://github.com/jordanch)",
    "JP Erasmus <jp@flamelink.io> (http://jperasmus.me)"
  ],
  "bugs": {
